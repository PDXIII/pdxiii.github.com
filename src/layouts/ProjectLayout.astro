---
import DefaultLayout from "./DefaultLayout.astro";
import MarkdownContainer from "../components/MarkdownContainer.astro";
const { frontmatter } = Astro.props;
---

<DefaultLayout title={frontmatter.title}>
  <MarkdownContainer>
    {frontmatter.titleImg && <img src={frontmatter.titleImg} alt="" />}
    <slot />
  </MarkdownContainer>
</DefaultLayout>
